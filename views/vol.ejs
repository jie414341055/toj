<div id="searchResultRow" class="row" style="display:none">
	<table id="searchResult" class="table table-striped table-centered">
		<thead>
			<tr>
				<th class="header-status">
				</th>
				<th class="header-id">
					<div id="searchTitleBtn" class="" order='asc'><a href="#">ID</a></div>
				</th>
				<th class="header-title">
					<div id="searchTitleBtn" class="" order='asc'><a href="#">Title</a></div>
				</th>
				<th class="header-source">
					<div id="searchTitleBtn" class="" order='asc'><a href="#">Source</a></div>
				</th>
				<th class="header-submit">
					<div id="titleBtn" class="" order='asc'><a href="#">Total</a></div>
				</th>
				<th class="header-ac">
					<div id="titleBtn" class="" order='asc'><a href="#">AC</a></div>
				</th>
				<th class="header-oj">
					<div id="searchFreshBtn" class="" order='asc'><a href="#">OJ</a></div>
				</th>
				<th class="header-ac">
					<div id="searchAcrateBtn" class="" order='asc'><a href="#">AC Ratio</a></div>
				</th>
			</tr>
		</thead>
		<tbody>
		</tbody>
	</table>
</div>
<div id="problemListRow" class="row">
	<ul class="pagination">
		<li class="disabled"><a href="#">&laquo;</a></li>
		<% for(var i = 1;i <= 10; ++i) { %>
		<% if (i == fvol_num) { %>
		<li class="active"><a href="#"><%= fvol_num %> <span class="sr-only">(current)</span></a></li>
		<% } else { %>
		<li><a href="/Problem/vol<%= i %>"><%= i %></a></li>
		<% } %>
		<% } %>
		<li><a href="#">&raquo;</a></li>
	</ul>
	<table id="problemList" class="table table-striped table-centered">
		<thead>
			<tr>
				<th class="header-status">
				</th>
				<th class="header-id">
					<div id="titleBtn" class="" order='asc'><a href="#">ID</a></div>
				</th>
				<th class="header-title">
					<div id="titleBtn" class="" order='asc'><a href="#">Title</a></div>
				</th>
				<th class="header-source">
					<div id="searchTitleBtn" class="" order='asc'><a href="#">Source</a></div>
				</th>
				<th class="header-submit">
					<div id="titleBtn" class="" order='asc'><a href="#">Total</a></div>
				</th>
				<th class="header-ac">
					<div id="titleBtn" class="" order='asc'><a href="#">AC</a></div>
				</th>
				<th class="header-oj">
					<div id="freshBtn" class="" order='asc'><a href="#">OJ</a></div>
				</th>
				<th class="header-ac">
					<div id="acrateBtn" class="" order='asc'><a href="#">AC Ratio</a></div>
				</th>
			</tr>
		</thead>
		<tbody>
			<% fprobs.forEach(function(prob, index) { %>
			<tr>
				<span class="ac"> </span>
				<td>
					<td><%= prob.pid %></td>
					<td><a href="/ShowProblems/<%= prob.pid %>"><%= prob.title %></a></td>
					<td><%= prob.source %></td>
					<td><%= prob.vtotal_submit + prob.total_submit %></td>
					<td><%= prob.vtotal_ac + prob.total_ac %></td>
					<td><%= prob.oj %></td>
					<td><%= Math.round(prob.vtotal_ac/prob.vtotal_submit*10000)/100 %>&#37;</td>
				</td>	
			</tr>
			<% }); %>
				
		</tbody>
	</table>
	<ul class="pagination">
		<li class="disabled"><a href="#">&laquo;</a></li>
		<% for(var i = 1;i <= 10; ++i) { %>
		<% if (i == fvol_num) { %>
		<li class="active"><a href="#"><%= fvol_num %> <span class="sr-only">(current)</span></a></li>
		<% } else { %>
		<li><a href="/Problem/vol<%= i %>"><%= i %></a></li>
		<% } %>
		<% } %>
		<li><a href="#">&raquo;</a></li>
	</ul>
</div>
